<view class="tabs">
    <view class="tabs_title">
        <view class="tabs_item {{item.isActive?'active':''}}" wx:for="{{tabs}}" wx:key="id" bind:tap="handleItemTab"
        data-index="{{index}}">
            {{item.value}}
        </view>
    </view> 
    <view class="tabs_content"></view>
</view>